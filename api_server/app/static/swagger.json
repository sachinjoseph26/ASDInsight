            "schema": {
              "type": "object",
              "properties": {
                "error": {
                  "type": "string",
                  "description": "Error message"
                }
              }
            }
          }
        }
      }
    },
    "/eye-tracking/extract-eye-features": {
      "post": {
        "summary": "Extract eye features from images in directory",
